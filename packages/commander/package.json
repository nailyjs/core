{
  "name": "@nailyjs/commander",
  "description": "Naily.js Commander",
  "version": "0.33.0",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.mjs",
  "types": "lib/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/esm/common/index.mjs",
      "require": "./lib/cjs/common/index.js",
      "types": "./lib/types/common/index.d.ts"
    },
    "./backend": {
      "import": "./lib/esm/backend/index.mjs",
      "require": "./lib/cjs/backend/index.js",
      "types": "./lib/types/backend/index.d.ts"
    }
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "naily-component"
  ],
  "scripts": {
    "build": "naily build --type lib",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "reflect-metadata": "^0.2.1",
    "commander": "^12.0.0",
    "@nailyjs/core": "workspace:*"
  }
}
